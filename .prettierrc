{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,
  "quoteProps": "consistent",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    },
    {
      "files": "src/content/blog/*.mdx",
      "options": {
        "embeddedLanguageFormatting": "off"
      }
    }
  ]
}
